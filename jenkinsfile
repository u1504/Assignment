pipeline{
  agent any
  stages{
    stage('terraform init'){
      steps{
        sh "terraform init"
      }
    }
   stages{
    stage('terraform plan'){
      steps{
        sh "terraform plan"
      }
    }
    stage('terraform apply'){
      steps{
        sh returnStatus: true, script: 'terraform apply -auto-approve'
      }
    }
  }
}
